@article{idGrowthPublicInvestment,
	title = {Growth, public investment and corruption with failing institutions},
	abstract = {Corruption is thought to prevent poor countries from catching up with richer ones. We analyze one channel through which corruption hampers growth: public investment can be distorted in favor of speciﬁc types of spending for which rent-seeking is easier and better concealed. To study this distortion, we propose a dynamic model where households vote for the composition of public spending, subject to an incentive constraint reﬂecting individuals’ choice between productive activity and rent-seeking. In equilibrium, the structure of public investment is determined by the predatory technology and the distribution of political power. Among diﬀerent regimes, the model shows a possible scenario of distortion without corruption in which there is no eﬀective corruption but the possibility of corruption still distorts the allocation of public investment. We test the implications of the model on a set of countries using a two-stage least squares estimation. We ﬁnd that developing countries with high predatory technology invest more in housing and physical capital in comparison with health and education. The reverse is true for developed countries.},
	language = {en},
	author = {Id, HAL},
	pages = {42},
	file = {Id - Growth, public investment and corruption with fail.pdf:C\:\\Users\\Jose Luis\\Zotero\\storage\\AD8JCPD5\\Id - Growth, public investment and corruption with fail.pdf:application/pdf},
}

@article{haqueWhyDoesPublic2014,
	title = {Why does {Public} {Investment} {Fail} to {Raise} {Economic} {Growth}? {The} {Role} of {Corruption}},
	volume = {83},
	shorttitle = {Why does {Public} {Investment} {Fail} to {Raise} {Economic} {Growth}?},
	doi = {10.1111/manc.12068},
	abstract = {In an endogenous growth model with information asymmetry between the government and the bureaucracy, the bureaucrats can falsely report of high-quality high-cost procurement, while providing low-quality low-cost product. This reduces the quality of public services, but inflates the public spending, which in effect reduces growth. We test our results by using three-stage least squares method in a panel set up for a system of four equations on growth, public investment, corruption and private investment. Our primary results are twofold. First, corruption increases public investment. Second, corruption reduces the returns to public investment and makes it ineffective in raising economic growth.},
	journal = {The Manchester School},
	author = {Haque, M. and Kneller, Richard},
	month = sep,
	year = {2014},
}

